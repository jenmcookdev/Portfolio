    <!--Contact section-->
    <section id="contact" class="contact">
        <!--Block content-->
        <div class="block-content">


            <!--Block contact-->
            <div class="block-contact">
                <div id="map" class="block-teaser block-background-image large overlay" data-background="/Content/img/contact/shuttlecock-kc.jpg">

                </div>

                <div class="contact-overlay bg-grey-dark">

                    <div class="block-title mb-60">
                        <h2 class="title white">Send a Message</h2>
                    </div>

                    <div class="block-form">
                        @using (Html.BeginForm("Contact", "Home", FormMethod.Post, new { @class = "contact-form clearfix", role = "form" }))
                        {
                            @Html.AntiForgeryToken()
                            @Html.TextBoxFor((model => model.name), new
                       {
                           placeholder = "Name *",
                           id = "name",
                           name = "name"
                       });
                            @Html.TextBoxFor((model => model.email), new
                       {
                           placeholder = "Email *",
                           id = "email",
                           name = "email"
                       });
                            @Html.TextBoxFor((model => model.subject), new
                       {
                           placeholder = "Subject *",
                           id = "subject",
                           name = "subject"
                       });
                            @Html.TextAreaFor((model => model.message), new
                       {
                           placeholder = "Message *",
                           cols = 45,
                           rows = 10,
                           id = "message",
                           name = "message"
                       });
                            <input type="submit" value="Send Message" id="btnSubmit" class="but opc white submit">
                        }
                        <!--Contact form message-->
                        <div><h2>@ViewBag.ErrorMessage</h2></div>
                        <!--End contact form message-->

                    </div>

                </div>
            </div>
            <!--End block contact-->

        </div>
        <!--End block content-->
    </section>
